      "tags": [],
      "implicitDependencies": ["landing-page"]
    },
    "access": { "tags": [] },
    "access-e2e": { "tags": [], "implicitDependencies": ["access"] },
    "data-access": { "tags": [] },
    "shared-ui-vue-landings": { "tags": [] },
